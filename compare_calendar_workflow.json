    {
      "parameters": {
        "functionCode": "\nconst csvItems = items[0].json.data;\nconst jsonItems = items[1].json;\n\nconst normalize = (entry) => ({\n  date: entry.date,\n  t: entry.t,\n  h: entry.h,\n  n: entry.n,\n});\n\nconst csvData = csvItems.map(normalize).sort((a, b) => a.date.localeCompare(b.date));\nconst jsonData = jsonItems.map(normalize).sort((a, b) => a.date.localeCompare(b.date));\n\nconst isEqual = JSON.stringify(csvData) === JSON.stringify(jsonData);\n\nreturn [\n  {\n    json: {\n      result: isEqual ? \"✅ Данные совпадают\" : \"❌ Обнаружены различия\",\n      csvLength: csvData.length,\n      jsonLength: jsonData.length,\n    }\n  }\n];\n"
      },
